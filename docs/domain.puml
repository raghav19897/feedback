@startuml
entity "Question" as question {
  *id: number
  text: string

}

entity "Option" as option {
  *id: number
  text: string
  value: number
  ---
  question_id:number <<FK>>
}

entity "Response" as response {
  *id: number
  response: number
  question_id: number
  submit_date: date
}
question }|-- option
response }|-- question
@enduml
